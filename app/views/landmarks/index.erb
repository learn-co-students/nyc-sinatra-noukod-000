<h2>All Historical Landmarks</h2>

<!-- add content here -->
<table>
  <thead>
    <th>Name</th>
    <th>Year Completed</th>
  </thead>
  <tbody>
    <% Landmark.all.each do |landmark| %>
    <tr>
        <td align="center"><a href="/landmarks/<%= landmark.id %>"><%= landmark.name %></a></td>

        <td align="center"><%= landmark.year_completed %></td>
    </tr>
    <% end %>
  </tbody>
</table>

